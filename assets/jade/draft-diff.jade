extends layouts/base
include mixins/_view_helpers

block vars
  - htmlClass = "html-draft-editor"
  - pageTitle = "!Draft changes | Objective[8]"


block headerScripts

block content

  .guidance-buttons.clj-guidance-buttons
    a.button-hide-guidance-text.clj-close-link(href="!draft")
      i.fa.fa-arrow-left
      =" "
      span(data-l8n="content:draft-diff/close") !Back to draft

  .draft-editor

    .draft-editor-document
      p.version(data-l8n="content:draft-diff/previous-version-title") !Previous version
      article.draft-preview-document.clj-add-previous-draft
